{
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["node"],
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@configs": ["src/configs"],
      "@configs/*": ["src/configs/*"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@containers": ["src/containers"],
      "@containers/*": ["src/containers/*"],
      "@layouts": ["src/layouts"],
      "@layouts/*": ["src/layouts/*"],
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"]
    }
  },
  "include": ["src", "src/**/*", ".eslintrc.config.cjs", "vite.config.ts"],
  "files": [],
  "references": [{ "path": "./tsconfig.node.json" }]
}
